          <JSR223PreProcessor guiclass="TestBeanGUI" testclass="JSR223PreProcessor" testname="UserId" enabled="true">
            <stringProp name="scriptLanguage">groovy</stringProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="script">ArrayList&lt;Integer&gt; Users = new ArrayList&lt;Integer&gt;()
int getIteration = vars.getIteration().toInteger()
Random randomGenerator = new Random()
def iteration = props.get(&quot;iteration&quot;)
def getUsers = props.get(&quot;Users&quot;)
int random

if ( iteration == null || iteration &lt; getIteration || getUsers.size() &lt; 1){
	while (Users.size() &lt;= 100) {
		RandUser = randomGenerator.nextInt(1000)
	    if (!Users.contains(RandUser)) {
	        Users.add(RandUser)
	    }
	}
	iteration = getIteration
	props.put(&quot;iteration&quot;,iteration)
	props.put(&quot;Users&quot;,Users)
log.info(&quot; Users: &quot;+props.get(&quot;Users&quot;).toString())
}


</stringProp>
          </JSR223PreProcessor>
